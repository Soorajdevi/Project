        <script>

       // Get the selected time range from the dropdown
       const timeRangeDropdown = document.getElementById('timeRangeDropdown');
timeRangeDropdown.addEventListener('change', async () => {
  const selectedTimeRange = timeRangeDropdown.value;
  let labels = [];
  let data = [];

  if (selectedTimeRange === 'weekly') {
    // Fetch weekly data from the backend
    const weeklyData = await fetchWeeklyData();
    labels = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'];
    data = weeklyData.map(day => day.totalSales);
  } else if (selectedTimeRange === 'monthly') {
    // Fetch monthly data from the backend
    const monthlyData = await fetchMonthlyData();
    const currentMonth = new Date().getMonth();
    labels = getMonthLabels(currentMonth);
    data = monthlyData.slice(0, currentMonth + 1);
  } else if (selectedTimeRange === 'yearly') {
    // Fetch yearly data from the backend
    const yearlyData = await fetchYearlyData();
    labels = ['2018','2019','2020','2021','2022','2023'];
    data = yearlyData;
  }

  const chartData = {
    labels: labels,
    datasets: [{
      label: '# of Sales',
      data: data,
      backgroundColor: 'rgba(255, 99, 132, 0.2)',
      borderColor: 'rgba(255, 99, 132, 1)',
      borderWidth: 1,
      fill: false
    }]
  };

  updateChart(chartData);
});

async function fetchWeeklyData() {
  const response = await fetch('/admin/week'); // Replace with your actual API endpoint for weekly data
  const data = await response.json();
  return data;
}

async function fetchMonthlyData() {
  const response = await fetch('/admin/month'); // Replace with your actual API endpoint for monthly data
  const data = await response.json();
  return data;
}

async function fetchYearlyData() {
  const response = await fetch('/admin/year'); // Replace with your actual API endpoint for yearly data
  const data = await response.json();
  return data;
}

// Function to update the chart with new data
function updateChart(data) {
  // Retrieve the chart element
  const lineChart = document.getElementById('lineChart');

  // Create the chart using Chart.js library
  new Chart(lineChart, {
    type: 'line',
    data: data,
    options: {
      // Add any additional options and styling as needed
    }
  });
}

// Function to get the labels for the monthly data
function getMonthLabels(currentMonth) {
  const monthLabels = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
  return monthLabels.slice(0, currentMonth + 1);
}

  </script>









